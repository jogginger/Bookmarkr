{
  "rules": [
    {
      "rule": "convert_require",
      "current": {
        "name": "path",
        "sources": {
          "@pkg": "Packages",
          "@src": "src",
          "@components": "src/Components"
        }
      },
      "target": {
        "name": "roblox",
        "rojo_sourcemap": "./sourcemap.json",
        "indexing_style": "find_first_child"
      }
    },
    "compute_expression",
    "convert_index_to_field",
    "filter_after_early_return",
    "remove_empty_do",
    "remove_nil_declaration",
    "remove_unused_if_branch",
    "remove_unused_while"
  ]
}
